{
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "婷宝美业"
      }
    },
    {
      "path": "pages/reservation/index",
      "style": {
        "navigationBarTitleText": "预约"
      },
      "lazyCodeLoading": "requiredComponents"
    },
    {
      "path": "pages/my/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "我的"
      }
    },
    {
      "path": "pages/project/index",
      "style": {
        "navigationStyle": "custom",
        "navigationBarTitleText": "服务项目"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  // TabBar
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#FE9393",
    "backgroundColor": "#fff",
    "borderStyle": "white",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/index/index",
        "iconPath": "static/tabbar/icon-index.png",
        "selectedIconPath": "static/tabbar/icon-index-active.png"
      },
      {
        "text": "预约",
        "pagePath": "pages/reservation/index",
        "iconPath": "static/tabbar/icon-reservation.png",
        "selectedIconPath": "static/tabbar/icon-reservation-active.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/my/index",
        "iconPath": "static/tabbar/icon-my.png",
        "selectedIconPath": "static/tabbar/icon-my-active.png"
      }
    ]
  }
}
